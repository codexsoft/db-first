{
    "name": "codexsoft/db-first",
    "license": "MIT",
    "description": "Database-first approach tool based on Doctrine2",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "doctrine/orm": "^2.6",
        "doctrine/migrations": "^1.5.0",
        "symfony/filesystem": "~3.4|~4.0|~5.0",
        "martin-georgiev/postgresql-for-doctrine": "0.10.*",
        "codexsoft/operations": "dev-master",
        "ramsey/uuid-doctrine": "^1.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/dotenv": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "": ["src/", "vendor/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": ["tests/unit/"]
        }
    },
  "bin": [
    "bin/codexsoft.dbfirst"
  ]
}
