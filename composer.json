{
    "name": "codexsoft/db-first",
    "license": "MIT",
    "description": "Database-first approach tool based on Doctrine2",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.4.0 || ^8.0",
        "ext-mbstring": "*",
        "codexsoft/code-classes": "^1.0",
        "codexsoft/code-files": "^1.0",
        "codexsoft/shortcut-tab-constant": "^1.0",
        "doctrine/inflector": "^2.0",
        "doctrine/orm": "^2.6",
        "martin-georgiev/postgresql-for-doctrine": "^1.3",
        "psr/log": "^1.1",
        "ramsey/uuid-doctrine": "^1.4",
        "symfony/console": "^5.2",
        "symfony/filesystem": "~3.4|~4.0|~5.0",
        "voku/stringy": "^6.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/dotenv": "^4.0|^5.0"
    },
    "suggest": {
        "ramsey/uuid-doctrine": "Allows the use of Ramsey\\Uuid\\Uuid as Doctrine field type.",
        "martin-georgiev/postgresql-for-doctrine": "Provides Doctrine support for some specific PostgreSQL 9.4+ features"
    },
    "autoload": {
        "psr-4": {
            "CodexSoft\\DatabaseFirst\\": "src/",
            "": ["vendor/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": ["tests/unit/"]
        }
    },
  "bin": [
    "bin/codexsoft.dbfirst"
  ]
}
